{
  "name": "tear-utilities",
  "version": "0.0.1",
  "description": "Utility bot for Tear Paradise",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run lint && npm run prettier && tsc",
    "lint": "eslint src/**/*.ts --fix",
    "prettier": "prettier --write src/**/*.ts",
    "test": "eslint src && prettier --check src/**/*.ts && tsc --noEmit",
    "start": "npm run build && node dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NotReallyEight/tear-utilities.git"
  },
  "keywords": [
    "utility-bot"
  ],
  "author": "NotReallyEight",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/NotReallyEight/tear-utilities/issues"
  },
  "homepage": "https://github.com/NotReallyEight/tear-utilities#readme",
  "dependencies": {
    "@discordjs/rest": "^0.2.0-canary.0",
    "@types/node": "^16.11.18",
    "chalk": "^4.1.2",
    "discord.js": "^13.5.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.5.0",
    "prettier": "^2.5.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^5.8.1"
  }
}
